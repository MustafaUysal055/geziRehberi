<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/arkaplan"
    tools:context=".istanbul1">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="181dp"
        android:layout_height="24dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="4dp"
        android:contextClickable="false"
        android:text="Dolmabahçe Sarayı"
        android:textAlignment="center"
        android:textSize="20dp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/dolmabahce"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/geri"
        android:layout_width="60dp"
        android:layout_height="52dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/arkaplan"
        android:onClick="geri"
        android:src="@drawable/geri"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ayasofya"
        android:layout_width="152dp"
        android:layout_height="96dp"
        android:layout_marginStart="6dp"
        android:onClick="ayasofya"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/ayasofya" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="154dp"
        android:layout_height="27dp"
        android:layout_marginTop="20dp"
        android:contextClickable="false"
        android:text="Ayasofya Camii"
        android:textAlignment="center"
        android:textSize="20dp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/ayasofya"
        app:layout_constraintTop_toBottomOf="@+id/geri"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/dolmabahce"
        android:layout_width="152dp"
        android:layout_height="96dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="32dp"
        android:onClick="dolmabahce"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:srcCompat="@drawable/dolmabahce" />

    <ImageView
        android:id="@+id/topkapi"
        android:layout_width="162dp"
        android:layout_height="101dp"
        android:layout_marginTop="9dp"
        android:onClick="topkapi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:srcCompat="@drawable/topkapi" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="154dp"
        android:layout_height="27dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="32dp"
        android:contextClickable="false"
        android:text="Topkapı Sarayı"
        android:textAlignment="center"
        android:textSize="20dp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ayasofya" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="179dp"
        android:layout_height="27dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="16dp"
        android:contextClickable="false"
        android:text="Sultanahmet Camii"
        android:textAlignment="center"
        android:textSize="20dp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dolmabahce" />

    <ImageView
        android:id="@+id/sultanahmet"
        android:layout_width="167dp"
        android:layout_height="110dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="19dp"
        android:onClick="sultanahmet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:srcCompat="@drawable/sultanahmet" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="154dp"
        android:layout_height="31dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="36dp"
        android:text="Rumeli Hisarı"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topkapi" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="154dp"
        android:layout_height="31dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="19dp"
        android:text="Kapalıçarşı"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sultanahmet" />

    <ImageView
        android:id="@+id/rumeli"
        android:layout_width="174dp"
        android:layout_height="106dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="188dp"
        android:onClick="rumeli"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        app:layout_constraintVertical_bias="0.142"
        app:srcCompat="@drawable/rumeli" />

    <ImageView
        android:id="@+id/kapalicarsi"
        android:layout_width="165dp"
        android:layout_height="110dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="177dp"
        android:onClick="kapalicarsi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/kapalicarsi" />

</androidx.constraintlayout.widget.ConstraintLayout>